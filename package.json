{
    "name": "my-massa-sc",
    "version": "0.0.2",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "asp --summary",
        "build": "massa-as-compile",
        "clean": "rimraf build",
        "deploy": "tsx src/deploy.ts",
        "recurring": "tsx src/recurring.ts",
        "deploy:dex": "tsx src/deployDEX.ts",
        "hello": "tsx src/hello.ts",
        "prettier": "prettier '**/src/**/*.ts' --check && as-prettier --check assembly",
        "prettier:fix": "prettier '**/src/**/*.ts' --write && as-prettier --write assembly",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "fmt:check": "npm run prettier && npm run lint",
        "fmt": "npm run prettier:fix && npm run lint:fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@as-pect/cli": "^8.1.0",
        "@assemblyscript/loader": "^0.27.37",
        "@massalabs/as-types": "^2.1.0",
        "@massalabs/eslint-config": "^0.0.11",
        "@massalabs/massa-as-sdk": "^3.0.2",
        "@massalabs/massa-sc-compiler": "0.2.1-dev.20240725151426",
        "@massalabs/massa-web3": "^5.3.0",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@massalabs/sc-standards": "^1.3.0",
        "@types/node": "^18.19.130",
        "as-bignum": "^0.3.1",
        "assemblyscript": "^0.27.37",
        "assemblyscript-prettier": "^1.0.7",
        "dotenv": "^16.6.1",
        "prettier": "^2.8.8",
        "tslib": "^2.8.1",
        "tsx": "^4.20.6",
        "typescript": "^4.9.5"
    },
    "overrides": {
        "visitor-as": {
            "assemblyscript": "$assemblyscript"
        },
        "@massalabs/as-types": {
            "as-bignum": "$as-bignum"
        },
        "@massalabs/sc-standards": {
            "as-bignum": "$as-bignum"
        }
    },
    "type": "module",
    "prettier": "@massalabs/prettier-config-as",
    "engines": {
        "node": ">=16"
    }
}
